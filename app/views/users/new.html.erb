<h1>New user</h1>
<div class="col-md-12"> 
<div class="col-md-2">
<%= form_for @user do |f| %>
<%= f.label :name, class: "form-group" %> 
<%= f.text_field :name %> 
<%= f.label :favorite_color %>
<%= f.text_field :favorite_color %> 
<%= f.label :favorite_movie %> 
<%= f.text_field :favorite_movie %> 
  <%= f.submit "create user!", class: "btn-sm btn-info" %> 
<% end %> 
</div>
</div>

<%= link_to 'Back', users_path %>
